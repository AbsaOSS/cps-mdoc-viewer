{
  "name": "cps-mdoc-viewer-srcs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run minify-assets-generator && ng build cps-mdoc-viewer --configuration=production",
    "build:example-app": "ng build --project example-app --base-href '/cps-mdoc-viewer/'",
    "watch": "ng build cps-mdoc-viewer --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && prettier --check .",
    "lint:library": "ng lint cps-mdoc-viewer && prettier --check ./projects/cps-mdoc-viewer",
    "lint:app": "ng lint example-app && prettier --check ./projects/example-app",
    "lint:fix": "ng lint --fix && prettier --write .",
    "generate-assets": "node projects/cps-mdoc-viewer/src/assets-generator/generate-assets.js --input=projects/example-app/src/assets/categories --serveUrl=assets/categories --mainTs=projects/example-app/src/main.ts",
    "watch-assets": "chokidar --initial 'projects/example-app/src/assets/categories/**/*' -c 'npm run generate-assets' -i 'projects/example-app/src/assets/categories/categories.json'",
    "minify-assets-generator": "esbuild --minify projects/cps-mdoc-viewer/src/assets-generator/generate-assets.js --outfile=projects/cps-mdoc-viewer/src/assets-generator/generate-assets.min.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.9",
    "@angular/common": "^20.3.9",
    "@angular/compiler": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@angular/router": "^20.3.9",
    "bootstrap": "^5.3.8",
    "cps-ui-kit": "^20.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "marked": "^16.4.1",
    "ngx-markdown": "^20.1.0",
    "primeicons": "^7.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-eslint/builder": "20.4.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/schematics": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.9",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-license-header": "^0.8.0",
    "jest": "^29.7.0",
    "ng-mocks": "^14.14.0",
    "ng-packagr": "^20.3.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3"
  }
}
